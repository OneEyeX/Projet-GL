import lombok.ToString;

@ToString
@JsonIgnoreProperties(ignoreUnknown = true)
public class CommissionAffectationMembreModel{
	
	/**
	 * 
	 */
	private Commission  commission;
	private Membre membre;
	//@JsonInclude(JsonInclude.Include.NON_EMPTY)
	private DomaineActivite selectedDomaineActivite;
	private TypeMembreCommissionMarche selectedPosteCM;
	private TypeMembreCellulePassation selectedPosteCP;
	private Date dateAffectation;
  
  


	public CommissionAffectationMembreModel(Commission commission, DomaineActivite selectedDomaineActivite,
		 Membre membre, Date dateAffectation, TypeMembreCommissionMarche selectedPosteCM,
			TypeMembreCellulePassation selectedPosteCP) {

		this.commission = commission;
		this.selectedDomaineActivite = selectedDomaineActivite;
	//	this.selectedPoste = selectedPoste;
		this.membre = membre;
		this.dateAffectation = dateAffectation;
		this.selectedPosteCM = selectedPosteCM;
		this.selectedPosteCP = selectedPosteCP;
	}


	public CommissionAffectationMembreModel() {
		// TODO Auto-generated constructor stub
	}

	
	
