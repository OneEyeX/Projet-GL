		return commission;
	}


	public void setCommission(Commission commission) {
		this.commission = commission;
	}


	public DomaineActivite getSelectedDomaineActivite() {
		return selectedDomaineActivite;
	}


	public void setSelectedDomaineActivite(DomaineActivite selectedDomaineActivite) {
		this.selectedDomaineActivite = selectedDomaineActivite;
	}



	public Membre getMembre() {
		return membre;
	}

	public void setMembre(Membre membre) {
		this.membre = membre;
	}

	public Date getDateAffectation() {
		return dateAffectation;
	}

	public void setDateAffectation(Date dateAffectation) {
		this.dateAffectation = dateAffectation;
	}

	public TypeMembreCommissionMarche getSelectedPosteCM() {
		return selectedPosteCM;
	}

	public void setSelectedPosteCM(TypeMembreCommissionMarche selectedPosteCM) {
		this.selectedPosteCM = selectedPosteCM;
	}

	public TypeMembreCellulePassation getSelectedPosteCP() {
		return selectedPosteCP;
	}

	public void setSelectedPosteCP(TypeMembreCellulePassation selectedPosteCP) {
		this.selectedPosteCP = selectedPosteCP;
	}


	
	

